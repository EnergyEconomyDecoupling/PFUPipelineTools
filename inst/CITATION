  textVersion  =
  paste("Matthew Kuperus Heun (2023).",
        "PFUPipelineTools: Tools for PFU Pipelines",
        "R package version 0.1.2.",
        "https://github.com/EnergyEconomyDecoupling/PFUAggDatabase")
)
